{
  "name": "1balita",
  "version": "1.0.0",
  "description": "1Balita is a Philippine online news aggregator from different local news providers.",
  "main": "server/index.ts",
  "scripts": {
    "dev:server": "ts-node --files  --transpile-only --project ./tsconfig.server.json ./server/index.ts",
    "dev": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling npx nodemon --watch 'server/**/*.ts' --exec 'npm run dev:server'",
    "lint": "eslint './**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint './**/*.{ts,tsx,js,jsx}' --fix",
    "prod:server": "NODE_ENV=production ts-node --files --transpile-only --project ./tsconfig.server.json ./server/index.ts",
    "test": "jest --coverage --verbose",
    "start": "npm run prod:server",
    "build": "next build",
    "nodemon": "npx nodemon"
  },
  "keywords": [
    "twitter",
    "news",
    "feed",
    "aggregation"
  ],
  "author": "albertarvesu",
  "license": "ISC",
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "cron": "^1.7.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "isomorphic-unfetch": "^3.0.0",
    "module-alias": "^2.2.2",
    "mongoose": "^5.7.12",
    "morgan": "^1.9.1",
    "next": "^9.1.4",
    "next-offline": "^4.0.6",
    "nodemon": "^2.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "styled-components": "^4.4.1",
    "ts-node": "^8.5.2",
    "twitter": "^1.7.1",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/mongoose": "^5.5.32",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.11",
    "@types/react": "^16.9.12",
    "@types/styled-components": "^4.4.0",
    "@types/twitter": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0"
  }
}
